{
    "compilerOptions": {
        "target": "esnext",
        "lib": [
            "esnext",
            "dom"
        ],
        "module": "esnext",
        "moduleResolution": "node",
        "noEmit": true,
        "strict": false,
        "skipLibCheck": true,
        "allowJs": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "types": []
    },
    "exclude": [
        "node_modules"
    ]
}